<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Include security-related bug patterns -->
    <Match>
        <Bug category="SECURITY"/>
    </Match>
    
    <!-- Include SQL injection patterns -->
    <Match>
        <Bug pattern="SQL_NONCONSTANT_STRING_PASSED_TO_EXECUTE"/>
    </Match>
    
    <!-- Include command injection patterns -->
    <Match>
        <Bug pattern="COMMAND_INJECTION"/>
    </Match>
    
    <!-- Include path traversal patterns -->
    <Match>
        <Bug pattern="PATH_TRAVERSAL_IN"/>
    </Match>
    
    <!-- Include XSS patterns -->
    <Match>
        <Bug pattern="XSS_REQUEST_PARAMETER_TO_SEND_ERROR"/>
    </Match>
    
    <!-- Include weak cryptography patterns -->
    <Match>
        <Bug pattern="WEAK_MESSAGE_DIGEST_MD5"/>
    </Match>
    <Match>
        <Bug pattern="WEAK_MESSAGE_DIGEST_SHA1"/>
    </Match>
    
    <!-- Include insecure random patterns -->
    <Match>
        <Bug pattern="PREDICTABLE_RANDOM"/>
    </Match>
</FindBugsFilter>